<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Rectangle?>

<AnchorPane fx:id="root" prefHeight="680.0" prefWidth="1200.0" styleClass="bodybg" stylesheets="@Style.css" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="FXML.StartMenuController">
    <children>
      <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#6fa8dd" height="680.0" opacity="0.0" stroke="BLACK" strokeType="INSIDE" width="1200.0" />
      <Button fx:id="newButton" layoutX="525.0" layoutY="190.0" mnemonicParsing="false" onAction="#newGame" prefHeight="40.0" prefWidth="150.0" text="New game" />
      <Button fx:id="loadButton" layoutX="525.0" layoutY="290.0" mnemonicParsing="false" onAction="#loadGame" prefHeight="40.0" prefWidth="150.0" text="Load game" />
      <Button fx:id="closeButton" layoutX="525.0" layoutY="460.0" mnemonicParsing="false" onAction="#exitGame" prefHeight="40.0" prefWidth="150.0" text="Exit game" />
   </children>
</AnchorPane>
